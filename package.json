{
  "name": "joy-con-element",
  "version": "1.0.1",
  "description": "Stencil Component Starter",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepublishOnly": "stencil build",
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "unswitch": "^1.4.0"
  },
  "devDependencies": {
    "@stencil/core": "^0.11.3",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mattdsteele/joy-con-element.git"
  },
  "author": "mattdsteele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattdsteele/joy-con-element"
  },
  "homepage": "https://github.com/mattdsteele/joy-con-element",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
